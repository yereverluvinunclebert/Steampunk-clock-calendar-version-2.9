                }
                if (system.event.keyCode==37) {
                      //RWD track
                      if (debugFlg === 1) {print("%KON-I-INFO, left key "+system.event.keyCode);};
                      //sliderSetOnMouseUp();
                }
               ]]>
        </action>

        <action trigger="onload">	<![CDATA[
		include("steampunkclock.js");
		include("vitality.js");
	        include("Resources/Licence/Licence.js");
		startup();
	// ]]> </action>

	<action trigger="onPreferencesChanged">
                changePrefs();
        </action>


        <timer name="clockTimer">
		<interval>1</interval>
		<ticking>true</ticking>
		<onTimerFired> <![CDATA[
			updateTime();
		// ]]> </onTimerFired>
	</timer>
        <timer name="counterTimer">
		<interval>.1</interval>
		<ticking>false</ticking>
		<onTimerFired> <![CDATA[
			changecounter();
		// ]]> </onTimerFired>
	</timer>

        <timer name="tickTimer">
		<interval>1</interval>
		<ticking>false</ticking>
		<onTimerFired> <![CDATA[
			tick();
		// ]]> </onTimerFired>
	</timer>
         <timer name="bellTimer">
		<interval>2</interval>
		<ticking>false</ticking>
		<onTimerFired> <![CDATA[
			tollbell();
		// ]]> </onTimerFired>
	</timer>
        <timer name="pendulumTimer">
		<interval>0.01</interval>
		<ticking>false</ticking>
		<onTimerFired> <![CDATA[
                         if (system.cpu.sys + system.cpu.user <= preferences.cpuPref.value)
                         {
                             acceleration = gravity * angle;
                             velocity += acceleration;
                             angle += velocity;
                             pendulumSet.rotation = angle +180;
                         }
		// ]]> </onTimerFired>
	</timer>
        <timer name="alarmTimer">
		<interval>1</interval>
		<ticking>false</ticking>
		<onTimerFired> <![CDATA[
			ringalarmbell();
		// ]]> </onTimerFired>
	</timer>

        <timer name="timeMachineTimer">
		<interval>0.5</interval>
		<ticking>false</ticking>
		<onTimerFired> <![CDATA[
			timeMachineTimerTicking();
		// ]]> </onTimerFired>
	</timer>


</widget>