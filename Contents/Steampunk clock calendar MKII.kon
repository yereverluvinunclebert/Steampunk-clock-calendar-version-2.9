		<group>config</group>
		<type>popup</type>
		<option>raised</option>
		<option>lowered</option>
		<option>-</option>
		<defaultValue>raised</defaultValue>
		<description>Raise or lower the time display screen.</description>
	</preference>

	<preference name="rangeMathPrefFlg">
		<title>Slider Range or Maths:</title>
		<group>config</group>
		<type>popup</type>
		<option>range</option>
		<option>maths</option>
		<option>-</option>
		<defaultValue>maths</defaultValue>
		<description>Range or Maths for the slider. Range may be less cpu intensive on low power systems when operating the time slider.</description>
	</preference>

	<preference name="showHelpPref">
		<title>Startup Help Page:</title>
		<group>config</group>
		<type>popup</type>
		<option>display</option>
		<option>hide</option>
		<defaultValue>display</defaultValue>
		<description>When the clock starts it always displays a help page. Here you can toggle that page on/off.</description>
	</preference>

        <action trigger="onKeyDown">
              <![CDATA[
                if (system.event.keyCode==16) {
                    if (debugFlg === 1) {print("%KON-I-INFO, pressing SHIFT "+system.event.keyCode);};
                    shiftKeyFlag = 1;
                }
                if (system.event.keyCode==116) {
                    if (debugFlg === 1) {print("%KON-I-INFO, pressing F5 "+system.event.keyCode);};
                    reloadWidget();
                }
                if (system.event.keyCode==39) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing right key "+system.event.keyCode);};
                      if (sliderMechanismStatus == "released") {
                        sliderSet.hOffset = sliderSet.hOffset  + (5 * (clockScale / 100));
                        sliderSetClicked = true;
                        constrainSliderSet();
                      }
                }
                if (system.event.keyCode==37) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing left key "+system.event.keyCode);};
                      if (sliderMechanismStatus == "released") {
                        sliderSet.hOffset = sliderSet.hOffset - (5 * (clockScale / 100));
                        sliderSetClicked = true;
                        constrainSliderSet();
                      }
                }
                if (system.event.keyCode==65) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the A key "+system.event.keyCode);};
                      brassbuttonAOnClick();
                }
                if (system.event.keyCode==72) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the H key "+system.event.keyCode);};
                      helpdropdownmove();
                }
                if (system.event.keyCode==76) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the L key "+system.event.keyCode);};
                      brassbuttonLOnMouseDown();
                }
                if (system.event.keyCode==77) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the M key "+system.event.keyCode);};
                       togglechimes();
                }
                if (system.event.keyCode==80) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the P key "+system.event.keyCode);};
                      togglethependulum();
                }
                if (system.event.keyCode==84) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the T key "+system.event.keyCode);};
                      toggletimemachine();
                }
                if (system.event.keyCode==83) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the S key "+system.event.keyCode);};
                      screentoggle();
                }
                if (system.event.keyCode==87) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the W key "+system.event.keyCode);};
                      weekdaytoggle();
                }
                if (system.event.keyCode==66) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the B key "+system.event.keyCode);};
                      togglebackscreen();
                }
                if (system.event.keyCode==49) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the 1 key "+system.event.keyCode);};
                      flag01OnMouseDown();
                }
                if (system.event.keyCode==50) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the 2 key "+system.event.keyCode);};
                      flag02OnMouseDown();
                }
                if (system.event.keyCode==51) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the 3 key "+system.event.keyCode);};
                      flag03OnMouseDown();
                }
                if (system.event.keyCode==52) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the 4 key "+system.event.keyCode);};
                      flag04OnMouseDown();
                }
                if (system.event.keyCode==53) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the 5 key "+system.event.keyCode);};
                      flag05OnMouseDown();
                }
                if (system.event.keyCode==38) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the UP arrow key "+system.event.keyCode);};
                      handleUpArrow();
                }
                if (system.event.keyCode==40) {
                      if (debugFlg === 1) {print("%KON-I-INFO, pressing the DOWN arrow key "+system.event.keyCode);};
                      handleDownArrow();
                }
               ]]>
        </action>
        <action trigger="onKeyUp">
